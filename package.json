{
  "name": "six-cities-simple",
  "version": "1.0.0",
  "description": "Проект «Шесть городов (простой)» от HTML Academy",
  "keywords": [
    "rest",
    "api",
    "node.js"
  ],
  "main": "main.js",
  "scripts": {
    "start": "npm run build && node ./dist/main.js",
    "build": "npm run clean && npm run compile",
    "lint": "eslint src/ --ext .ts",
    "compile": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "ts": "ts-node",
    "server": "json-server --watch db.json --port 8080",
    "generate": "node ./dist/cli --generate 1000 ./dist/mock/main.tsv http://localhost:8080/offerSets",
    "import": "node ./dist/cli --import ./dist/mock/main.tsv"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "axios": "0.27.2",
    "eslint": "^8.12.0",
    "eslint-config-htmlacademy": "^5.1.2",
    "json-server": "0.17.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "type": "module",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.0"
  },
  "dependencies": {
    "@ngneat/falso": "6.1.0",
    "app-root-path": "3.1.0"
  }
}
